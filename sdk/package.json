{
  "name": "@lograven/sdk",
  "version": "0.1.44",
  "description": "Universal SDK for error monitoring with support for backend and frontend JavaScript environments.",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./react": {
      "import": "./dist/esm/react.js",
      "require": "./dist/cjs/react.js",
      "types": "./dist/types/react.d.ts"
    },
    "./browser": {
      "import": "./dist/browser/lograven.min.js"
    },
    "./vue": {
      "import": "./dist/esm/adapters/vue.js",
      "require": "./dist/cjs/adapters/vue.js",
      "types": "./dist/types/adapters/vue.d.ts"
    },
    "./angular": {
      "import": "./dist/esm/adapters/angular.js",
      "require": "./dist/cjs/adapters/angular.js",
      "types": "./dist/types/adapters/angular.d.ts"
    },
    "./fastify": {
      "import": "./dist/esm/adapters/fastify.js",
      "require": "./dist/cjs/adapters/fastify.js",
      "types": "./dist/types/adapters/fastify.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf dist",
    "build": "rollup -c rollup.config.mjs && tsc -p tsconfig.build.json",
    "prepare": "npm run build",
    "release": "npm version patch && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bhaktitud/lograven-sdk.git"
  },
  "keywords": [
    "sdk",
    "error-monitoring",
    "typescript",
    "universal",
    "lograven"
  ],
  "author": "Bhakti Budiman Novanda",
  "license": "MIT",
  "peerDependencies": {
    "@nestjs/common": "^9.0.0 || ^10.0.0 || ^11.0.0",
    "express": "^4.18.0 || ^5.0.0",
    "fastify": "^3.0.0 || ^4.0.0",
    "react": "^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^17.0.0 || ^18.0.0 || ^19.0.0",
    "rxjs": "^7.0.0",
    "vue": "^2.0.0 || ^3.0.0"
  },
  "peerDependenciesMeta": {
    "@nestjs/common": {
      "optional": true
    },
    "express": {
      "optional": true
    },
    "fastify": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "rxjs": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@nestjs/common": "^11.1.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/express": "^4.17.21",
    "@types/react": "^19.1.4",
    "core-js": "^3.36.0",
    "fastify": "^4.25.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "regenerator-runtime": "^0.14.1",
    "rollup": "^4.40.2",
    "rxjs": "^7.8.2",
    "typescript": "^5.3.3",
    "vue": "^3.4.21"
  },
  "engines": {
    "node": ">=18"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not IE 11",
    "> 0.5%",
    "Firefox ESR"
  ]
}
